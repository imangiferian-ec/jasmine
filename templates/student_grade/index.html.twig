{% extends 'base.html.twig' %}

{% block title %}StudentGrade index{% endblock %}

{% block body %}
    <h1>StudentGrade index</h1>

    <table class="table" id="{{ module}}Table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for student_grade in student_grades %}
            <tr>
                <td>{{ student_grade.id }}</td>
                <td>
                    <a href="{{ path('student_grade_show', {'id': student_grade.id}) }}">show</a>
                    <a href="{{ path('student_grade_edit', {'id': student_grade.id}) }}">edit</a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('student_grade_new') }}">Create new</a>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('vendor/datatables/jquery.dataTables.min.js') }}"></script>
    <script>
        var module = '{{module}}';
    </script>
    <script src="{{ asset('js/datatables.js') }}"></script>
{% endblock %}