{% extends 'base.html.twig' %}

{% block title %}AddedProfLoad index{% endblock %}

{% block body %}
    <h1>AddedProfLoad index</h1>

    <table class="table" id="{{ module}}Table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for added_prof_load in added_prof_loads %}
            <tr>
                <td>{{ added_prof_load.id }}</td>
                <td>
                    <a href="{{ path('added_prof_load_show', {'id': added_prof_load.id}) }}">show</a>
                    <a href="{{ path('added_prof_load_edit', {'id': added_prof_load.id}) }}">edit</a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('added_prof_load_new') }}">Create new</a>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('vendor/datatables/jquery.dataTables.min.js') }}"></script>
    <script>
        var module = '{{module}}';
    </script>
    <script src="{{ asset('js/datatables.js') }}"></script>
{% endblock %}
