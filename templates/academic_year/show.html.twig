{% extends 'base.html.twig' %}
{% block body %}
    <h1>Academic Year</h1>
    <a href="{{ path('academic_year_index') }}">back to list</a> |
    <a href="{{ path('academic_year_edit', {'id': academic_year.id}) }}">edit</a>
    <hr>
    <div class="row">
      <div class="col-md-8 offset-md-2">
        <table class="table">
        <tbody>
        <tr>
        <th>Start Year</th>
        <td>{{ academic_year.ayStartYear }}</td>
        </tr>
        <tr>
        <th>End Year</th>
        <td>{{ academic_year.ayEndYear }}</td>
        </tr>
        <tr>
        <th>Status</th>
        <td>{{ academic_year.status == 'a' ? 'Active' : 'Inactive' }}</td>
        </tr>
        <tr>
        <th>Is Accepting Examinee?</th>
        <td>{{ academic_year.isAcceptingExaminee ? 'Yes' : 'No' }}</td>
        </tr>
        </tbody>
        </table>
      </div>
    </div>
    <hr>
    {{ include('academic_year/_delete_form.html.twig') }}
{% endblock %}
